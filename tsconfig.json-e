{
    "include": ["src/**/*", "dev_types/**/*"],
    "compilerOptions": {
        "rootDir": "src",
        "typeRoots": ["dev_types", "./node_modules/@types"],
        "outDir": "build",
        "target": "ES2022",
        "module": "Node16",
        "moduleResolution": "Node16",
        "resolveJsonModule": true,
        "jsx": "react",
        "jsxFactory": "jsxCreateElement",
        "jsxFragmentFactory": "NOT_IMPLEMENTED",
        "strictNullChecks": true,
        "noImplicitReturns": true,
        "noFallthroughCasesInSwitch": true,
        "noImplicitAny": true
    }
}
