{
    "name": "jpdb-browser-reader",
    "version": "0.0.1",
    "scripts": {
        "build": "prettier --write . && concurrently 'tsc --listFiles' 'copy-and-watch \"src/**/!(*.mts|*.ts)\" dist/'",
        "watch": "concurrently 'tsc --listFiles --watch' 'copy-and-watch --watch \"src/**/!(*.mts|*.ts)\" dist/'",
        "lint": "prettier --write . && eslint ."
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "5.54.0",
        "@typescript-eslint/parser": "5.54.0",
        "concurrently": "7.6.0",
        "copy-and-watch": "0.1.6",
        "eslint": "8.35.0",
        "prettier": "2.8.4",
        "typescript": "4.9.5"
    }
}
